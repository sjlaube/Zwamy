<div class="players">
   <table>
   <%@players.each do|player|%>
   <tr><td><%=player["nickname"]%></td>
   <td><%=player["account"]%></td>
   
           <td><%= form_tag("deletePlayer", method: "post") do %>
                <%= hidden_field_tag("account", player["account"])%>
                <%= submit_tag("Delete") %>
            <% end %>
        </td> 
    </tr>   
   <%end%>
   </table>
</div>



